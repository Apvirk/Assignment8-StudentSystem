@model IEnumerable<StudentSystem.Branch>

@{
    ViewBag.Title = "Branch Details";
}

<h2>Branch Details</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Trade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Branch1)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Trade)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Branch1)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
